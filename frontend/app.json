{
  "expo": {
    "name": "bkeepit",
    "slug": "bkeepit",
    "scheme": "bkeepit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-bg.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.poneworx.bkeepit",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.poneworx.bkeepit",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": []
    },
    "web": {
      "favicon": "./assets/images/app-icon.png"
    },
    "plugins": [],
    "extra": {
      "apiBaseUrl": "https://api.bkeepit.com"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}


